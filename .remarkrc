{
  "plugins": {
    "toc": {
      "tight": true
    },
    "slug": {
      "library": "github"
    },
    "lint": {
      "no-html": false,
      "list-item-spacing": false,
      "list-item-indent": "mixed",
      "code-block-style": "fenced",
      "maximum-heading-length": 80,
      "no-heading-punctuation": false,
      "no-missing-blank-lines": false
    },
    "validate-links": true
  },
  "settings": {
    "listItemIndent": "1",
    "bullet": "*"
  }
}
